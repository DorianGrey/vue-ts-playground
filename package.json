{
  "name": "vue-ts-playground",
  "description": "Playground project using Vue.js and typescript",
  "author": {
    "name": "Christian Linne",
    "email": "ShadowVampire@web.de"
  },
  "scripts": {
    "start": "parcel public/index.html -d build -p 3000",
    "build": "parcel build public/index.html -d build",
    "serve": "node scripts/serve.js",
    "test": "node scripts/test.js",
    "test:ci": "node scripts/test.js --no-cache --coverage",
    "test:debug": "node --inspect --inspect-brk scripts/test.js",
    "test:ci:debug": "node --inspect --inspect-brk scripts/test.js --no-cache --coverage",
    "format": "prettier --write \"{src,scripts,config}/**/*.{js,ts}\"",
    "precommit": "lint-staged",
    "prepush": "npm run test:ci"
  },
  "lint-staged": {
    "{src,scripts,config}/**/*.{js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "av-ts": "^0.9.3",
    "core-js": "^2.5.7",
    "lodash-es": "^4.17.8",
    "sanitize.css": "^5.0.0",
    "tslib": "^1.9.2",
    "vee-validate": "2.0.9",
    "vue": "^2.5.16",
    "vue-carousel-3d": "^0.1.20",
    "vue-i18n": "^7.8.0",
    "vue-router": "^3.0.1",
    "vuetify": "^1.0.19",
    "vuex": "^3.0.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@types/jest": "^23.0.2",
    "@types/lodash-es": "4.17.0",
    "@types/node": "^10.3.4",
    "@types/webfontloader": "^1.6.29",
    "@vue/component-compiler-utils": "^2.0.0",
    "@vue/test-utils": "^1.0.0-beta.18",
    "autoprefixer": "^8.6.2",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chalk": "^2.4.1",
    "connect-history-api-fallback": "^1.5.0",
    "cssnano": "^3.10.0",
    "fs-extra": "^6.0.1",
    "globby": "^8.0.1",
    "husky": "^0.14.3",
    "internal-ip": "^3.0.1",
    "jest": "^23.1.0",
    "koa": "^2.5.1",
    "koa-compress": "^3.0.0",
    "koa-connect": "^2.0.1",
    "koa-static": "^5.0.0",
    "lint-staged": "^7.2.0",
    "lodash": "^4.17.5",
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.9.2",
    "postcss-flexbugs-fixes": "^3.3.1",
    "prettier": "^1.13.5",
    "purify-css": "^1.2.5",
    "shelljs": "^0.8.2",
    "signale": "^1.2.0",
    "strip-ansi": "^4.0.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-formatter-pretty": "^1.0.3",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.1.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "2.9.1",
    "vue-hot-reload-api": "^2.3.0",
    "vue-jest": "^2.6.0",
    "vue-template-compiler": "^2.5.16"
  }
}
