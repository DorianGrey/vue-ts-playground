{
  "name": "vue-ts-playground",
  "description": "Playground project using Vue.js and typescript",
  "author": {
    "name": "Christian Linne",
    "email": "ShadowVampire@web.de"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "start:local": "node scripts/start.js --local-network",
    "build": "node scripts/build.js",
    "serve": "node scripts/serve.js",
    "test": "node scripts/test.js",
    "test:ci": "node scripts/test.js --no-cache --coverage",
    "test:debug": "node --inspect --inspect-brk scripts/test.js",
    "test:ci:debug": "node --inspect --inspect-brk scripts/test.js --no-cache --coverage",
    "format": "prettier --write \"{src,scripts,config}/**/*.{js,ts}\"",
    "precommit": "lint-staged",
    "prepush": "npm run test:ci"
  },
  "lint-staged": {
    "{src,scripts,config}/**/*.{js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
